	<description>Generates spaceship names.</description>
	<genre>Sci-Fi</genre>
	<category>Name</category>
	<supportsMaxLength>false</supportsMaxLength>
	<items>
		<item name="Start">[UpperCase][UpperCase]-[Digit][Digit] [Name]</item>
