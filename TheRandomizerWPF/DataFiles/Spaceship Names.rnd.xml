		<item name="Start">[UpperCase][UpperCase]-[Digit][Digit] [Class]</item>
		<item name="Start">The [Noun] [Class]</item>
		<item name="Start">The [Noun] [Size] [Class]</item>
		<item name="Start">[Name]'s [Noun]</item>
		<item name="Start">[Name]'s [Noun]</item>
		<item name="Start">The [Adjective] [Noun]</item>
		<item name="Start">The [Noun] of [Name]</item>
		<item name="Start">The [Name]</item>
		<item name="Start">The [Noun]</item>
		<item name="Start">[UpperCase][UpperCase][UpperCase] [Class]</item>
		
